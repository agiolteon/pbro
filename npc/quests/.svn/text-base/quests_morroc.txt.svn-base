moc_ruins,118,176,4	script	Alquimista	64,{
	mes "[Marius]";
	mes "Olá, novo cliente!";
	mes "Eu sei o que você vai falar. Você quer o Binóculos de mim, ceeeerto?";
	next;
	switch(select("O que é isso?:Fabricação:Ignore-o")) {
	case 1:
		mes "[Marius]";
		mes "Hmm~!";
		mes "Bem, o nome oficial para eles é ^FF0000Binóculos^000000! Um dispositivo óptico que funciona como um par de óculos campo, eles são projetados para utilização simultânea por ambos os olhos!";
		next;
		mes "[Marius]";
		mes "Eles são constituídos por dois pequenos telescópios ingressos com um único dispositivo. Você pode organizar as lentes para produzir visão estereoscópica.";
		next;
		mes "[Marius]";
		mes "Então?";
		mes "Não é que";
		mes "alguma coisa, hein?";
		mes "Muhahahahahaha!";
		mes ".....";
		next;
		mes "[Marius]";
		mes "Hm? Você não parece";
		mes "acreditar no que eu disse?";
		mes "Ah, imbecil. Isso é real.";
		next;
		mes "[Marius]";
		mes "Eu vou deixar você.";
		mes "Eu preciso dos seguintes itens...";
		mes "1 ^00FF00Quatro-Olhos^000000, 100 ^00FF00Aços^000000";
		mes "e ^00FF0050000 Zenys^000000!";
		close;
	case 2:
		if ((countitem(2243) > 0) && (countitem(999) > 99) && (Zeny >= 50000)) {
		mes "[Marius]";
		mes "Perfeito, perfeito!";
		mes "Agora a minha obra vai ser concluída!";
		mes "Muhahahaha!";
			next;
			delitem 2243,1;  //Spinning_Eyes
			delitem 999,100;  //Steel
			set Zeny,Zeny-50000;
			mes "[Marius]";
			mes "Aqui está!";
			mes "Binóculos!";
			getitem 2296,1; // Binoculars
			next;
			mes "[Marius]";
			mes "Basta lembrar ...";
			mes "Não olhe para onde você não possa, bom, pelo menos tente.";
			close;
		}
		else {
			mes "[Marius]";
			Mes "Argggghhhhhhh!";
			Mes "Você não trouxe os itens";
			Mes "Como você se atreve a";
			Mes "tentar me enganar?";
			close;
		}
	case 3:
		mes "[Marius]";
		Mes "Ei você ~!";
		mes "Agora você está na frente de Marius, um alquimista entre Alquimistas. Me ignorando, não é? Vamos lá, vou bater-te!";
		close;
	}
}

