moc_ruins,113,181,4	script	William	89,{
	mes "[William]";
	mes "Bem-vindo à";
	mes "Fábrica de ^3355FFPlacas^000000 MacMillan.";
	next;
	mes "[William]";
	mes "A minha família, o Clã MacMillan, vem produzindo profissionalmente placas de trânsito há de 250 anos. Atualmente, estamos com muitos pedidos de ^3355FF Placas de Pare ^000000.";
	next;
	switch(select("Posso pedir uma ^3355FFPlaca^000000?:Cancelar")) {
	case 1:
		mes "[William]";
		mes "Este é um ornamento único, a ^3355FFPlaca de Pare^000000, é um sinal de trânsito na estrada, que dobra como um ornamento cabelo! Isso, nós prometemos solenemente sobre a força de uma garantia de 100 anos.";
		next;
		mes "[William]";
		mes "Para produzir uma ^3355FFPlaca de Pare^000000, precisamos ^00FF0091100^000000 de zenys, 50 ^00FF00Troncos^000000 e 1 ^00FF00Tintura Preta^000000.";
		next;
		if ((countitem(1019) > 49) && (countitem(983) > 0) && (Zeny >= 91100)) {
			delitem 1019,50;  //Wooden_Block
			delitem 983,1;  //Black_Dyestuffs
			set Zeny,Zeny-91100;
			mes "[William]";
			mes "Aqui está a";
			mes "^3355FFPlaca de Pare^000000 que";
			mes "foi especialmente feita";
			mes "só para você!";
			mes "Obrigado pela visita!";
			getitem 2272,1; // Stop_Post
			close;
		} else {
			mes "[William]";
			mes "Então ...";
			mes "Quando você obter esses itens, vamos trocar eles pela Placa de Pare.";
			close;
		}
	case 2:
		mes "[William]";
		mes "De qualquer forma, obrigado por vir à Fábrica MacMillan, mas pense em comprar algo da próxima vez, sim?";
		close;
	}
}

